{
	"id": "SUSE-SU-2018:2907-1",
	"modified": "2018-09-27T14:02:27Z",
	"published": "2018-09-27T14:02:27Z",
	"related": [
		"CVE-2018-10902",
		"CVE-2018-10940",
		"CVE-2018-14634",
		"CVE-2018-14734",
		"CVE-2018-15572",
